{
  "name": "reactify",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "chokidar-cli": "^1.2.1",
    "concurrently": "^4.1.0",
    "node-sass": "^4.10.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1"
  },
  "scripts": {
    "prestart": "yarn sass:build",
    "sass:build": "node-sass-chokidar --include-path ./node_modules/ ./src/styles/sass/index.scss -o ./src/styles/css/",
    "sass:watch": "node-sass-chokidar --include-path ./node_modules/ ./src/styles/sass/index.scss -o ./src/styles/css/ --watch",
    "start": "react-scripts start",
    "build": "yarn sass:build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
